{
  "name": "vite-react-ts-eslint-prettier",
  "private": false,
  "version": "1.0.0",
  "homepage": "https://juniorbaymax.github.io/React_Typescript_vite",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 8080",
    "lint": "npx eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint \"./{src, cypress}/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./{src, cypress}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "test": "react-scripts test",
    "cypress:open": "cypress open",
    "prepare": "husky install",
    "tsc:check": "npm run tsc:check-app && npm run tsc:check-cypress",
    "tsc:check-app": "tsc --noEmit --project \"./tsconfig.json\"",
    "tsc:check-cypress": "tsc --noEmit --project \"./cypress/tsconfig.json\"",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "test:e2e": "cypress run --headed --config-file \"./cypress/config/cypress.config.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@percy/cypress": "^3.1.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@tabler/icons": "^2.11.0",
    "@testing-library/jest-dom": "github:testing-library/jest-dom",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "antd": "^5.3.3",
    "crypto-js": "^4.1.1",
    "formik": "^2.2.9",
    "framer-motion": "^10.10.0",
    "react": "^18.1.0",
    "react-apexcharts": "^1.4.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.1.0",
    "react-element-to-jsx-string": "^15.0.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-scripts": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "simplebar-react": "^3.2.3",
    "vite-plugin-eslint": "^1.8.1",
    "web-vitals": "^3.3.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.9",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.25",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cypress": "^12.11.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.6.2",
    "sass": "^1.59.3",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^3.6.0"
  }
}
